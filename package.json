{
    "name": "leetcode-dump",
    "version": "0.4.4",
    "description": "Dump your LeetCode solutions, and generate a static website to show them.",
    "main": "lib/index.js",
    "bin": {
        "leetcode-dump": "lib/leetcode-dump.js",
        "lcd": "lib/leetcode-dump.js"
    },
    "scripts": {
        "test": "jest --coverage",
        "dev": "tsup --watch",
        "build": "npm run format && tsup",
        "docs": "typedoc ./src/",
        "format": "prettier --write '**/*.{js,ts,jsx,tsx,json,yml,yaml,md,html}' '!lib' '!docs' '!leetcode' '!site' '!site_tmp' '!demo.yml'",
        "lint": "eslint ."
    },
    "keywords": [
        "leetcode",
        "dump",
        "solution"
    ],
    "author": "JacobLinCool <jacoblincool@gmail.com> (https://github.com/JacobLinCool)",
    "license": "MIT",
    "types": "lib/index.d.ts",
    "files": [
        "lib"
    ],
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^3.2.0",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.21",
        "@types/node-fetch": "2.5.12",
        "@types/turndown": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^5.13.0",
        "@typescript-eslint/parser": "^5.13.0",
        "eslint": "^8.10.0",
        "eslint-config-prettier": "^8.5.0",
        "jest": "^27.5.1",
        "prettier": "^2.5.1",
        "ts-jest": "^27.1.3",
        "tsup": "^5.12.0",
        "typedoc": "^0.22.13",
        "typescript": "^4.6.2"
    },
    "dependencies": {
        "@vuepress/plugin-medium-zoom": "^2.0.0-beta.36",
        "@vuepress/plugin-nprogress": "^2.0.0-beta.36",
        "@vuepress/plugin-search": "^2.0.0-beta.36",
        "chalk": "4.1.2",
        "commander": "9.0.0",
        "dotenv": "16.0.0",
        "fs-extra": "^10.0.1",
        "leetcode-query": "0.0.7",
        "node-fetch": "2.6.7",
        "ora": "5.4.1",
        "turndown": "^7.1.1",
        "vuepress": "^2.0.0-beta.36"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "webpack"
            ]
        }
    }
}
