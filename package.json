{
    "name": "leetcode-dump",
    "version": "0.4.7",
    "description": "Dump your LeetCode solutions, and generate a static website to show them.",
    "main": "lib/index.js",
    "bin": {
        "leetcode-dump": "lib/leetcode-dump.js",
        "lcd": "lib/leetcode-dump.js"
    },
    "scripts": {
        "test": "jest --coverage",
        "dev": "tsup --watch",
        "build": "npm run format && tsup",
        "docs": "typedoc ./src/",
        "format": "prettier --write '**/*.{js,ts,jsx,tsx,json,yml,yaml,md,html}' '!lib' '!docs' '!leetcode' '!site' '!site_tmp' '!demo.yml'",
        "lint": "eslint ."
    },
    "keywords": [
        "leetcode",
        "solution",
        "dump",
        "download",
        "export",
        "blog"
    ],
    "author": "JacobLinCool <jacoblincool@gmail.com> (https://github.com/JacobLinCool)",
    "license": "MIT",
    "types": "lib/index.d.ts",
    "files": [
        "lib"
    ],
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^3.2.0",
        "@types/jest": "^27.5.1",
        "@types/node": "^17.0.35",
        "@types/node-fetch": "2.5.12",
        "@types/turndown": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^5.25.0",
        "@typescript-eslint/parser": "^5.25.0",
        "eslint": "^8.15.0",
        "eslint-config-prettier": "^8.5.0",
        "jest": "^27.5.1",
        "prettier": "^2.6.2",
        "ts-jest": "^27.1.5",
        "tsup": "^5.12.8",
        "typedoc": "^0.22.15",
        "typescript": "^4.6.4"
    },
    "dependencies": {
        "@vuepress/client": "2.0.0-beta.45",
        "@vuepress/plugin-medium-zoom": "^2.0.0-beta.45",
        "@vuepress/plugin-nprogress": "^2.0.0-beta.45",
        "@vuepress/plugin-search": "^2.0.0-beta.45",
        "chalk": "4.1.2",
        "commander": "9.0.0",
        "dotenv": "16.0.0",
        "fs-extra": "^10.1.0",
        "leetcode-query": "^0.0.8",
        "node-fetch": "2.6.7",
        "ora": "5.4.1",
        "turndown": "^7.1.1",
        "vue": "^3.2.34",
        "vuepress": "^2.0.0-beta.45"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "webpack"
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/JacobLinCool/LeetCode-Dump.git"
    },
    "bugs": {
        "url": "https://github.com/JacobLinCool/LeetCode-Dump/issues"
    },
    "homepage": "https://github.com/JacobLinCool/LeetCode-Dump#readme"
}
