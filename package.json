{
    "name": "leetcode-dump",
    "version": "0.1.1",
    "description": "Dump Your LeetCode Solutions.",
    "main": "lib/index.js",
    "bin": {
        "leetcode-dump": "lib/index.js"
    },
    "scripts": {
        "test": "jest --coverage",
        "prepare": "npm run build && npm run docs",
        "dev": "tsup --watch",
        "build": "npm run format && tsup",
        "docs": "typedoc ./src/",
        "format": "prettier --write '**/*.{js,ts,jsx,tsx,json,yml,yaml,md,html}' '!lib' '!docs' '!leetcode'",
        "lint": "eslint ."
    },
    "keywords": [
        "leetcode",
        "dump"
    ],
    "author": "JacobLinCool <jacoblincool@gmail.com> (https://github.com/JacobLinCool)",
    "license": "MIT",
    "types": "lib/index.d.ts",
    "files": [
        "lib"
    ],
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^3.2.0",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.21",
        "@types/node-fetch": "2.5.12",
        "@typescript-eslint/eslint-plugin": "^5.13.0",
        "@typescript-eslint/parser": "^5.13.0",
        "eslint": "^8.10.0",
        "eslint-config-prettier": "^8.5.0",
        "jest": "^27.5.1",
        "prettier": "^2.5.1",
        "ts-jest": "^27.1.3",
        "tsup": "^5.12.0",
        "typedoc": "^0.22.13",
        "typescript": "^4.6.2"
    },
    "dependencies": {
        "chalk": "4.1.2",
        "commander": "9.0.0",
        "dotenv": "16.0.0",
        "leetcode-query": "0.0.7",
        "node-fetch": "2.6.7",
        "ora": "5.4.1"
    }
}
